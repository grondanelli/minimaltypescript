{
  "name": "mintypescript",
  "version": "0.0.0",
  "description": "Minimalistic TypeScript proyect",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/node": "~8.10.30",
    "prettier": "1.14.3",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-microsoft-contrib": "~5.2.1",
    "tsutils": "~3.0.0",
    "typescript": "~3.1.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "Run": "node .\\build\\main.js"
  },
  "author": "Gian Rondanelli",
  "license": "Apache-2.0",
  "dependencies": {
    "tslib": "~1.9.3"
  }
}
